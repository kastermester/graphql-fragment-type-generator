{
	"compilerOptions": {
		"strictNullChecks": true,
		"module": "commonjs",
		"target": "es5",
		"declaration": false,
		"noEmit": false,
		"newLine": "LF",
		"outDir": "../testlib",
		"noImplicitAny": true,
		"inlineSourceMap": true,
		"inlineSources": true,
		"sourceRoot": "..",
		"allowJs": false,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"lib": [
			"es2015"
		]
	},
	"include": [
		"../types/*.d.ts",
		"../src/**/*.ts",
		"**/*.ts"
	]
}
